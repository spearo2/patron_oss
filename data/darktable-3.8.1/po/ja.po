msgid "interface"
msgstr "インターフェース"

#: ../build/bin/preferences_gen.h:7308
msgid "sort built-in presets first"
msgstr "内蔵プリセットを先頭に並べる"

#: ../build/bin/preferences_gen.h:7322
msgid ""
