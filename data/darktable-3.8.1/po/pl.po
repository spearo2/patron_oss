#: ../src/iop/filmicrgb.c:3367 ../src/iop/filmicrgb.c:4068
msgid "display"
msgstr "wyjście"

#. axis legend
#: ../src/iop/filmicrgb.c:3376
msgid "(%)"
msgstr "(%)"

#. Page SCENE
#: ../src/iop/filmicrgb.c:3385 ../src/iop/filmicrgb.c:3905
