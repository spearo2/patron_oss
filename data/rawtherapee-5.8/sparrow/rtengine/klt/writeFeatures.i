static void _printFeatureTxt(
  FILE *fp,
  KLT_Feature feat,
  const char *format,
  const char type)
{
  (static_cast <bool> (type == 'f' || type == 'd') ? void (0) : __assert_fail ("type == 'f' || type == 'd'", "/root/oss-experiment/data/rawtherapee-5.8/rtengine/klt/writeFeatures.cc", 296, __extension__ __PRETTY_FUNCTION__));

  if (type == 'f')
    fprintf(fp, format, (float) feat->x, (float) feat->y, feat->val);
  else if (type == 'd') {

    float x = feat->x;
    float y = feat->y;
    if (x >= 0.0) x += 0.5;
    if (y >= 0.0) y += 0.5;
    fprintf(fp, format,
            (int) x, (int) y, feat->val);
  }
}


static void _printFeatureBin(
