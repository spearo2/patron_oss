static structureType _readHeader(
  FILE *fp,
  int *nFrames,
  int *nFeatures,
  int *binary)
{

  char line[100];
  structureType id;


  fread(line, sizeof(char), 6, fp);
  line[6] = 0;
  if (strcmp(line, binheader_fl) == 0) {
    (static_cast <bool> (nFeatures != nullptr) ? void (0) : __assert_fail ("nFeatures != nullptr", "/root/oss-experiment/data/rawtherapee-5.8/rtengine/klt/writeFeatures.cc", 478, __extension__ __PRETTY_FUNCTION__));
    fread(nFeatures, sizeof(int), 1, fp);
    *binary = 1;
    return FEATURE_LIST;
  } else if (strcmp(line, binheader_fh) == 0) {
    (static_cast <bool> (nFrames != nullptr) ? void (0) : __assert_fail ("nFrames != nullptr", "/root/oss-experiment/data/rawtherapee-5.8/rtengine/klt/writeFeatures.cc", 483, __extension__ __PRETTY_FUNCTION__));
    fread(nFrames, sizeof(int), 1, fp);
    *binary = 1;
    return FEATURE_HISTORY;
  } else if (strcmp(line, binheader_ft) == 0) {
    (static_cast <bool> (nFrames != nullptr) ? void (0) : __assert_fail ("nFrames != nullptr", "/root/oss-experiment/data/rawtherapee-5.8/rtengine/klt/writeFeatures.cc", 488, __extension__ __PRETTY_FUNCTION__));
    (static_cast <bool> (nFeatures != nullptr) ? void (0) : __assert_fail ("nFeatures != nullptr", "/root/oss-experiment/data/rawtherapee-5.8/rtengine/klt/writeFeatures.cc", 489, __extension__ __PRETTY_FUNCTION__));
    fread(nFrames, sizeof(int), 1, fp);
    fread(nFeatures, sizeof(int), 1, fp);
    *binary = 1;
    return FEATURE_TABLE;


  } else {
    rewind(fp);
    *binary = 0;
  }


  while (strcmp(line, warning_line) != 0) {
    fgets(line, 100, fp);
    if (feof(fp)) {
      KLTError("(_readFeatures) File is corrupted -- Couldn't find line:\n"
               "\t%s\n", warning_line);
      exit(1);
    }
  }


  while (fgetc(fp) != '-');
  while (fgetc(fp) != '\n');
  fgets(line, 100, fp);
  if (strcmp(line, "KLT Feature List\n") == 0) id = FEATURE_LIST;
  else if (strcmp(line, "KLT Feature History\n") == 0) id = FEATURE_HISTORY;
  else if (strcmp(line, "KLT Feature Table\n") == 0) id = FEATURE_TABLE;
  else {
    KLTError("(_readFeatures) File is corrupted -- (Not 'KLT Feature List', "
             "'KLT Feature History', or 'KLT Feature Table')");
    exit(1);
  }





  if ((id == FEATURE_LIST && nFeatures == nullptr) ||
      (id == FEATURE_HISTORY && nFrames == nullptr) ||
      (id == FEATURE_TABLE && (nFeatures == nullptr || nFrames == nullptr)))
    return id;


  while (fgetc(fp) != '-');
  while (fgetc(fp) != '\n');
  fscanf(fp, "%s", line);
  if (id == FEATURE_LIST) {
    if (strcmp(line, "nFeatures") != 0) {
      KLTError("(_readFeatures) File is corrupted -- "
               "(Expected 'nFeatures', found '%s' instead)", line);
      exit(1);
    }
  } else if (strcmp(line, "nFrames") != 0) {
    KLTError("(_readFeatures) File is corrupted -- "
             "(Expected 'nFrames', found '%s' instead)", line);
    exit(1);
  }
  fscanf(fp, "%s", line);
  if (strcmp(line, "=") != 0) {
    KLTError("(_readFeatures) File is corrupted -- "
             "(Expected '=', found '%s' instead)", line);
    exit(1);
  }
  if (id == FEATURE_LIST) fscanf(fp, "%d", nFeatures);
  else fscanf(fp, "%d", nFrames);


  if (id == FEATURE_TABLE) {
    fscanf(fp, "%s", line);
    if (strcmp(line, ",") != 0) {
      KLTError("(_readFeatures) File '%s' is corrupted -- "
               "(Expected 'comma', found '%s' instead)", line);
      exit(1);
    }
    fscanf(fp, "%s", line);
    if (strcmp(line, "nFeatures") != 0) {
      KLTError("(_readFeatures) File '%s' is corrupted -- "
               "(2 Expected 'nFeatures ', found '%s' instead)", line);
      exit(1);
    }
    fscanf(fp, "%s", line);
    if (strcmp(line, "=") != 0) {
      KLTError("(_readFeatures) File '%s' is corrupted -- "
               "(2 Expected '= ', found '%s' instead)", line);
      exit(1);
    }
    fscanf(fp, "%d", nFeatures);
  }


  while (fgetc(fp) != '-');
  while (fgetc(fp) != '\n');

  return id;

}


